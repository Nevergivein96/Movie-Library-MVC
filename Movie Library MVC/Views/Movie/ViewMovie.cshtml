@model Movie

<h3>@Model.MovieName Information</h3>

<table class="table">
    <tr>
        <th>Movie ID</th>
        <th>Movie Name</th>
        <th>Movie Genre</th>
        <th>Rating</th>
        <th>Rating Scale</th>
        <th>Series or Movie</th>
    </tr>
    <tr>
        <td>@Model.MovieID</td>
        <td>@Model.MovieName</td>
        <td>@Model.GenreName</td>
        <td>@Model.Rating</td>
        <td>@Model.RatingScale</td>
        <td>@Model.SeriesOrMovie</td>
    </tr>
</table>